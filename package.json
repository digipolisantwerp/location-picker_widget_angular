    "@types/proj4": "^2.5.2",
    "core-js": "^2.5.4",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ncp": "^2.0.0",
    "ng-packagr": "^15.2.2",
    "proj4": "^2.6.2",
    "protractor": "~7.0.0",
    "rxjs": "~6.6.7",
    "ts-node": "~7.0.0",
